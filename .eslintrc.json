{
   "env": {
      "browser": true,
      "es2021": true
   },
   "settings": {
      "react": {
         "version": "detect"
      }
   },
   "extends": [
      // "standard-with-typescript",
      "eslint:recommended",
      "plugin:react/recommended",
      //added below by me
      "plugin:@typescript-eslint/recommended",
      "prettier"
   ],
   "overrides": [],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["react", "@typescript-eslint"],
   "rules": {
      "react/react-in-jsx-scope": "off"
   }
}
